<h3> Student Form </h3>

<%= form_tag students_path do %>

  <p><%= label_tag :first_name, "First Name: " %> <%= text_field_tag :first_name, nil, id: :first_name %></p>
  <p><%= label_tag :last_name, "Last Name: " %> <%= text_field_tag :last_name, nil, id: :last_name %></p>

  <%= submit_tag "Submit Student" %>

<% end %>
